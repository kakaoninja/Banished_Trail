[application]

config/name="Banished Trail"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=480
window/size/height=270
window/vsync/use_vsync=false
window/ios/hide_home_indicator=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/smoothing/plugin.cfg" )

[global]

wind=false

[gui]

theme/custom="res://UIThemes/UITheme.tres"
theme/custom_font="res://UIThemes/Fonts/TextFont.tres"
timers/tooltip_delay_sec=0.0

